<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="PictionaryMusicalCliente.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
		</sectionGroup>
	</configSections>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<userSettings>
		<PictionaryMusicalCliente.Properties.Settings>
			<setting name="idiomaCodigo" serializeAs="String">
				<value>es-MX</value>
			</setting>
		</PictionaryMusicalCliente.Properties.Settings>
	</userSettings>

	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="BasicHttpBinding_IClasificacionManejador" />
				<binding name="BasicHttpBinding_IPerfilManejador" />
				<binding name="BasicHttpBinding_IInicioSesionManejador" />
				<binding name="BasicHttpBinding_ICuentaManejador" />
				<binding name="BasicHttpBinding_ICatalogoAvataresManejador" />
				<binding name="BasicHttpBinding_ICambioContrasenaManejador" />
				<binding name="BasicHttpBinding_ICodigoVerificacionManejador" />
			</basicHttpBinding>

			<netTcpBinding>
				<binding name="tcpNoSecurity">
					<security mode="None" />
				</binding>
			</netTcpBinding>

			<wsDualHttpBinding>
				<binding name="wsDualNoSecurity">
					<security mode="None" />
				</binding>
			</wsDualHttpBinding>
		</bindings>

		<client>
			<!-- Amigos -->
			<endpoint address="http://192.168.1.5:8515/Pictionary/Amigos/Amigos"
                      binding="wsDualHttpBinding" bindingConfiguration="wsDualNoSecurity"
                      contract="PictionaryServidorServicioAmigos.IAmigosManejador"
                      name="WSDualHttpBinding_IAmigosManejador" />
			<endpoint address="net.tcp://192.168.1.5:8516/Pictionary/Amigos/Amigos"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioAmigos.IAmigosManejador"
                      name="NetTcpBinding_IAmigosManejador" />

			<!-- Avatares -->
			<endpoint address="http://192.168.1.5:8505/Pictionary/Avatares/Avatares"
                      binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICatalogoAvataresManejador"
                      contract="PictionaryServidorServicioAvatares.ICatalogoAvataresManejador"
                      name="BasicHttpBinding_ICatalogoAvataresManejador" />
			<endpoint address="net.tcp://192.168.1.5:8506/Pictionary/Avatares/Avatares"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioAvatares.ICatalogoAvataresManejador"
                      name="NetTcpBinding_ICatalogoAvataresManejador" />

			<!-- Cambio de contraseña -->
			<endpoint address="http://192.168.1.5:8509/Pictionary/CambiarContrasena/CambiarContrasena"
                      binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICambioContrasenaManejador"
                      contract="PictionaryServidorServicioCambioContrasena.ICambioContrasenaManejador"
                      name="BasicHttpBinding_ICambioContrasenaManejador" />
			<endpoint address="net.tcp://192.168.1.5:8510/Pictionary/CambiarContrasena/CambiarContrasena"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioCambioContrasena.ICambioContrasenaManejador"
                      name="NetTcpBinding_ICambioContrasenaManejador" />

			<!-- Clasificación -->
			<endpoint address="http://192.168.1.5:8511/Pictionary/Clasificacion/Clasificacion"
                      binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IClasificacionManejador"
                      contract="PictionaryServidorServicioClasificacion.IClasificacionManejador"
                      name="BasicHttpBinding_IClasificacionManejador" />
			<endpoint address="net.tcp://192.168.1.5:8512/Pictionary/Clasificacion/Clasificacion"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioClasificacion.IClasificacionManejador"
                      name="NetTcpBinding_IClasificacionManejador" />

			<!-- Código verificación -->
			<endpoint address="http://192.168.1.5:8503/Pictionary/CodigoVerificacion/CodigoVerificacion"
                      binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICodigoVerificacionManejador"
                      contract="PictionaryServidorServicioCodigoVerificacion.ICodigoVerificacionManejador"
                      name="BasicHttpBinding_ICodigoVerificacionManejador" />
			<endpoint address="net.tcp://192.168.1.5:8504/Pictionary/CodigoVerificacion/CodigoVerificacion"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioCodigoVerificacion.ICodigoVerificacionManejador"
                      name="NetTcpBinding_ICodigoVerificacionManejador" />

			<!-- Cuenta -->
			<endpoint address="http://192.168.1.5:8501/Pictionary/Cuenta/Cuenta"
                      binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICuentaManejador"
                      contract="PictionaryServidorServicioCuenta.ICuentaManejador"
                      name="BasicHttpBinding_ICuentaManejador" />
			<endpoint address="net.tcp://192.168.1.5:8502/Pictionary/Cuenta/Cuenta"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioCuenta.ICuentaManejador"
                      name="NetTcpBinding_ICuentaManejador" />

			<!-- Inicio Sesión -->
			<endpoint address="http://192.168.1.5:8507/Pictionary/InicioSesion/InicioSesion"
                      binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IInicioSesionManejador"
                      contract="PictionaryServidorServicioInicioSesion.IInicioSesionManejador"
                      name="BasicHttpBinding_IInicioSesionManejador" />
			<endpoint address="net.tcp://192.168.1.5:8508/Pictionary/InicioSesion/InicioSesion"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioInicioSesion.IInicioSesionManejador"
                      name="NetTcpBinding_IInicioSesionManejador" />

			<!-- Lista de amigos -->
			<endpoint address="http://192.168.1.5:8517/Pictionary/ListaAmigos/ListaAmigos"
                      binding="wsDualHttpBinding" bindingConfiguration="wsDualNoSecurity"
                      contract="PictionaryServidorServicioListaAmigos.IListaAmigosManejador"
                      name="WSDualHttpBinding_IListaAmigosManejador" />
			<endpoint address="net.tcp://192.168.1.5:8518/Pictionary/ListaAmigos/ListaAmigos"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioListaAmigos.IListaAmigosManejador"
                      name="NetTcpBinding_IListaAmigosManejador" />

			<!-- Perfil -->
			<endpoint address="http://192.168.1.5:8513/Pictionary/Perfil/Perfil"
                      binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPerfilManejador"
                      contract="PictionaryServidorServicioPerfil.IPerfilManejador"
                      name="BasicHttpBinding_IPerfilManejador" />
			<endpoint address="net.tcp://192.168.1.5:8514/Pictionary/Perfil/Perfil"
                      binding="netTcpBinding" bindingConfiguration="tcpNoSecurity"
                      contract="PictionaryServidorServicioPerfil.IPerfilManejador"
                      name="NetTcpBinding_IPerfilManejador" />
		</client>
	</system.serviceModel>
</configuration>
