<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>

	<system.serviceModel>
		<bindings>
			<basicHttpBinding>
				<binding name="BasicHttpBinding_IClasificacionManejador" />
				<binding name="BasicHttpBinding_IPerfilManejador" />
				<binding name="BasicHttpBinding_IInicioSesionManejador" />
				<binding name="BasicHttpBinding_ICuentaManejador" />
				<binding name="BasicHttpBinding_ICatalogoAvataresManejador" />
				<binding name="BasicHttpBinding_ICambioContrasenaManejador" />
				<binding name="BasicHttpBinding_ICodigoVerificacionManejador" />
			</basicHttpBinding>

			<netTcpBinding>
				<binding name="NetTcpBinding_Default">
					<security mode="None" />
				</binding>
			</netTcpBinding>

			<wsDualHttpBinding>
				<binding name="WSDualHttpBinding_Default">
					<security mode="None" />
				</binding>
			</wsDualHttpBinding>
		</bindings>

		<client>
			<!-- ===================== -->
			<!--   ENDPOINTS HTTP      -->
			<!-- ===================== -->
			<endpoint address="http://192.168.1.19:8501/Pictionary/Cuenta/Cuenta"
					  binding="basicHttpBinding"
					  bindingConfiguration="BasicHttpBinding_ICuentaManejador"
					  contract="PictionaryServidorServicioCuenta.ICuentaManejador"
					  name="HttpCuentaEndpoint" />

			<endpoint address="http://192.168.1.19:8503/Pictionary/CodigoVerificacion/CodigoVerificacion"
					  binding="basicHttpBinding"
					  bindingConfiguration="BasicHttpBinding_ICodigoVerificacionManejador"
					  contract="PictionaryServidorServicioCodigoVerificacion.ICodigoVerificacionManejador"
					  name="HttpCodigoVerificacionEndpoint" />

			<endpoint address="http://192.168.1.19:8505/Pictionary/Avatares/Avatares"
					  binding="basicHttpBinding"
					  bindingConfiguration="BasicHttpBinding_ICatalogoAvataresManejador"
					  contract="PictionaryServidorServicioAvatares.ICatalogoAvataresManejador"
					  name="HttpAvataresEndpoint" />

			<endpoint address="http://192.168.1.19:8507/Pictionary/InicioSesion/InicioSesion"
					  binding="basicHttpBinding"
					  bindingConfiguration="BasicHttpBinding_IInicioSesionManejador"
					  contract="PictionaryServidorServicioInicioSesion.IInicioSesionManejador"
					  name="HttpInicioSesionEndpoint" />

			<endpoint address="http://192.168.1.19:8509/Pictionary/CambiarContrasena/CambiarContrasena"
					  binding="basicHttpBinding"
					  bindingConfiguration="BasicHttpBinding_ICambioContrasenaManejador"
					  contract="PictionaryServidorServicioCambioContrasena.ICambioContrasenaManejador"
					  name="HttpCambioContrasenaEndpoint" />

			<endpoint address="http://192.168.1.19:8511/Pictionary/Clasificacion/Clasificacion"
					  binding="basicHttpBinding"
					  bindingConfiguration="BasicHttpBinding_IClasificacionManejador"
					  contract="PictionaryServidorServicioClasificacion.IClasificacionManejador"
					  name="HttpClasificacionEndpoint" />

			<endpoint address="http://192.168.1.19:8513/Pictionary/Perfil/Perfil"
					  binding="basicHttpBinding"
					  bindingConfiguration="BasicHttpBinding_IPerfilManejador"
					  contract="PictionaryServidorServicioPerfil.IPerfilManejador"
					  name="HttpPerfilEndpoint" />


			<!-- ===================== -->
			<!--   ENDPOINTS TCP       -->
			<!-- ===================== -->
			<endpoint address="net.tcp://192.168.1.19:8502/Pictionary/Cuenta/Cuenta"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioCuenta.ICuentaManejador"
					  name="TcpCuentaEndpoint" />

			<endpoint address="net.tcp://192.168.1.19:8504/Pictionary/CodigoVerificacion/CodigoVerificacion"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioCodigoVerificacion.ICodigoVerificacionManejador"
					  name="TcpCodigoVerificacionEndpoint" />

			<endpoint address="net.tcp://192.168.1.19:8506/Pictionary/Avatares/Avatares"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioAvatares.ICatalogoAvataresManejador"
					  name="TcpAvataresEndpoint" />

			<endpoint address="net.tcp://192.168.1.19:8508/Pictionary/InicioSesion/InicioSesion"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioInicioSesion.IInicioSesionManejador"
					  name="TcpInicioSesionEndpoint" />

			<endpoint address="net.tcp://192.168.1.19:8510/Pictionary/CambiarContrasena/CambiarContrasena"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioCambioContrasena.ICambioContrasenaManejador"
					  name="TcpCambioContrasenaEndpoint" />

			<endpoint address="net.tcp://192.168.1.19:8512/Pictionary/Clasificacion/Clasificacion"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioClasificacion.IClasificacionManejador"
					  name="TcpClasificacionEndpoint" />

			<endpoint address="net.tcp://192.168.1.19:8514/Pictionary/Perfil/Perfil"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioPerfil.IPerfilManejador"
					  name="TcpPerfilEndpoint" />

			<endpoint address="net.tcp://192.168.1.19:8516/Pictionary/Amigos/Amigos"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioAmigos.IAmigosManejador"
					  name="TcpAmigosEndpoint" />

			<endpoint address="net.tcp://192.168.1.19:8518/Pictionary/ListaAmigos/ListaAmigos"
					  binding="netTcpBinding" bindingConfiguration="NetTcpBinding_Default"
					  contract="PictionaryServidorServicioListaAmigos.IListaAmigosManejador"
					  name="TcpListaAmigosEndpoint" />
		</client>
	</system.serviceModel>
</configuration>
